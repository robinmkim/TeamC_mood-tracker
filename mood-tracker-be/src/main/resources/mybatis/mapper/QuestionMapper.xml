<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.teamc.moodtracker.dao.QuestionDao">
    <insert id="addQuestion" parameterType="qvo">

        insert into QNA_QUESTION values (QNA_QUESTION_seq.nextval, #{q_title}, #{q_content}, sysdate, #{q_hit}, #{q_writer})
    </insert>

</mapper>

